Proceso MateoGuznayeje5
	Definir d,n,f Como Real;
	Escribir 'Escriba El numerador ';
	Leer n;
	Escribir 'Escriba El denominador ';
	Leer d;
	f <- d/n;
	Si d=0 Entonces
		Escribir 'ERROR: El denominador debe ser mayor que 0';
	SiNo
		Escribir 'El resultado es ',f;
	FinSi
	Escribir 'Gracias por preferirnos ';
FinProceso
